#访问路径前缀
restBasePath: /api/v1

#服务启动端口号
server.port: 8084

#系统编码
systemCode: LCPM

#MQ编码
systemCharset: UTF-8

#mybatis的xml映射文件路径
mybatis.mapperLocations: classpath*:com/taikang/dic/ltci/mapper/*.xml

#需要注册别名的POJO类所在包
mybatis.typeAliasesPackage: com.taikang.dic.ltci.model

#数据库连接配置默认连接是开发环境
spring.datasource:
    initialize: false
    url: jdbc:mysql://10.130.206.191:3306/ltci_file_system
    username: tklcpm
    password: tklcpm
    
spring.datasource.tomcat:
    max-active: 50
    max-wait: 30000
    test-on-borrow:  true
    max-idle: 10
    min-idle: 5
    test-on-connect: true
    test-on-return: true
    test-while-idle: true
    validation-query: SELECT 1 FROM DUAL
    validation-query-timeout: -1

