	width:120px;
	background-color:#f4f4f4;
}
.value
{
	background-color:#FFFFFF;
	padding:5px;
	align:left; 
	align:left; 
}
.formtable .ui_buttons {
    white-space: nowrap;
    padding: 4px 8px;
