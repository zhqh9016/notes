package com.taikang.dic.ltci.model;

import java.io.Serializable;

/**
 * 返回信息实体
 *
 * @author itw_xuxf
 */
public class ErrorReason implements Serializable {
  private static final long serialVersionUID = 1L;
  //中心交易流水号
  private String transactionNum;
  //业务周期号
  private String cycleNum;
  //错误原因代码
  private String errorReasonCode;
  //错误原因描述
  private String errorReason;

  public String getTransactionNum() {
    return transactionNum;
  }

  public void setTransactionNum(String transactionNum) {
    this.transactionNum = transactionNum;
  }

  public String getCycleNum() {
    return cycleNum;
  }

  public void setCycleNum(String cycleNum) {
    this.cycleNum = cycleNum;
  }

  public String getErrorReasonCode() {
    return errorReasonCode;
  }

  public void setErrorReasonCode(String errorReasonCode) {
    this.errorReasonCode = errorReasonCode;
  }

  public String getErrorReason() {
    return errorReason;
  }

  public void setErrorReason(String errorReason) {
    this.errorReason = errorReason;
  }
}
